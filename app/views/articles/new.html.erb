<div class="container">
  <div class="header">
    <h1>Wikinimous</h1>
    <%= link_to "Home", articles_path, class: "btn btn-ghost" %>
  </div>
</div>

<div class="form">
<%= form_for(@article) do |f| %>
  <h2><%= f.label :title %></h2>
  <%= f.text_field :title %>
  <h2><%= f.label :content %></h2>
  <%= f.text_field :content %>
  <p>
  <%= f.submit %>
  </p>
<% end %>
</div>
